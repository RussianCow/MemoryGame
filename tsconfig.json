{
	"compilerOptions": {
		"target": "es6",
		"lib": [
			"dom",
			"dom.iterable",
			"esnext"
		],
		"module": "esnext",
		"moduleResolution": "node",
		"baseUrl": "./",
		"allowJs": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"noEmitOnError": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitAny": true,
		"resolveJsonModule": true,
		"strictNullChecks": true,
		"jsx": "react"
	},
	"exclude": [
		"node_modules"
	]
}
